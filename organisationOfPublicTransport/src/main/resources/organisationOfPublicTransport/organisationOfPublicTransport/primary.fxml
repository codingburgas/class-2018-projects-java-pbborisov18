<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="hbox" maxHeight="1080" maxWidth="1920" prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox HBox.hgrow="ALWAYS">
         <children>
            <ScrollPane VBox.vgrow="ALWAYS">
               <content>
                  <Label text="Busses" />
               </content>
            </ScrollPane>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
               <children>
                <Button mnemonicParsing="false" text="Broken Bus" />
                <Button mnemonicParsing="false" text="Fixed Bus" />
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox HBox.hgrow="ALWAYS">
         <children>
            <ScrollPane VBox.vgrow="ALWAYS">
              <content>
                  <Label text="Actions" />
              </content>
            </ScrollPane>
         </children>
      </VBox>
      <VBox HBox.hgrow="ALWAYS">
         <children>
            <ScrollPane VBox.vgrow="ALWAYS">
              <content>
                  <Label text="Routes" />
              </content>
            </ScrollPane>
            <HBox>
               <children>
                  <Button mnemonicParsing="false" text="Reroute" />
               </children>
            </HBox>
            <Label text="Status" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </children>
</HBox>
